#include <stdio.h>

int GetSet(int *data, int max_size);

int main() {
    int *data, num;
    num = GetSet( &data, 100 ) ;
    printf("เซต : ");
    for (int i = 0 ; i < num; i++) {
        printf("%d ", data[i] ) ;
    }
    printf( "\n" ) ;

    return 0;
}//end function

int GetSet(int *data, int max_size) {
    int num;

    printf("ป้อนจำนวนสมาชิกของเซต : ");
    scanf("%d", &num);

    for (int i = 0; i < num; i++) {
        printf("ป้อนสมาชิกที่ %d: ", i + 1);
        scanf("%d", &data[i]);
    }
    return num;
}
